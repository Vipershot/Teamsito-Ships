{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:jsx-a11y/recommend",
        "plugin:react-hooks/recommend",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks",
        "prettier",
        "material-ui"
    ],
    "rules": {
        "prettier/prettier": "error",
        "material-ui/rule-name": 2,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
